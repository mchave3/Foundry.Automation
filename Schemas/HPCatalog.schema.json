          },
          "architecture": {
            "type": [
              "string",
              "null"
            ]
          },
          "osId": {
            "type": [
              "string",
              "null"
            ]
          },
          "osName": {
            "type": [
              "string",
              "null"
            ]
          },
          "softPaqId": {
            "type": [
              "string",
              "null"
            ]
          },
          "softPaqVersion": {
            "type": [
              "string",
              "null"
            ]
          },
          "name": {
            "type": [
              "string",
              "null"
            ]
          },
          "category": {
            "type": [
              "string",
              "null"
            ]
          },
          "dateReleased": {
            "type": [
              "string",
              "null"
