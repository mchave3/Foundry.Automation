          },
          "sha256": {
            "type": [
              "string",
              "null"
            ]
          },
          "cvaFileUrl": {
            "type": [
              "string",
              "null"
            ]
          },
          "hsaCompliant": {
            "type": [
              "string",
              "null"
            ]
          },
          "winpeFamily": {
            "type": [
              "string",
